{
  "root": "services/service-a",
  "sourceRoot": "services/service-a/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/services/service-a",
        "main": "services/service-a/src/index.ts",
        "tsConfig": "services/service-a/tsconfig.app.json",
        "assets": ["services/service-a/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["services/service-a/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/services/service-a"],
      "options": {
        "jestConfig": "services/service-a/jest.config.js",
        "passWithNoTests": true
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "commitMessageFormat": "chore(${projectName}): release version ${version}"
      }
    }
  },
  "tags": []
}
