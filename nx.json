{
  "extends": "@nrwl/workspace/presets/core.json",
  "npmScope": "prototype-nx",
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "package": [
      {
        "target": "package",
        "projects": "dependencies"
      }
    ],
    "version": [
      {
        "target": "version",
        "projects": "dependencies"
      }
    ]
  },
  "workspaceLayout": {
    "appsDir": "services",
    "libsDir": "libs"
  }
}
